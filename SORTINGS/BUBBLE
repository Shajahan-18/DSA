BUBBLE SORT

class Solution {
    public int[] bubbleSort(int[] nums) {
        int n=nums.length;
        for(int j=0;j<n;j++){

        for(int i=0;i<n-1-j;i++){
            int curr=nums[i];
            int next=nums[i+1];
            if(curr>next){
                int temp=nums[i];
                nums[i]=nums[i+1];
                nums[i+1]=temp;
            }
        }
        }
        return nums;
    }
}

                

                

           
